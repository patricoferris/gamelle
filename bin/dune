(executable
 (public_name gamelle)
 (name gamelle_bin)
 (package gamelle)
 (libraries
  gamelle_template
  unix
  threads
  str
  cmdliner
  dynlink
  imagelib
  imagelib.unix
  (select
   asset_editor.ml
   from
   (gamelle.sdl -> asset_editor.available.ml)
   (!gamelle.sdl -> asset_editor.unavailable.ml))
  (select
   hotreload.ml
   from
   (inotify gamelle.sdl -> hotreload.available.ml)
   (!inotify -> hotreload.unavailable.ml))))
